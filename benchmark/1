#!/bin/bash
# usage
# 

model_name="pytorch_db_res18"

mode=$1
batch_size=$2

IFS=" "
batch_list=(${batch_size})

for batch in ${batch_list[@]}; do

    if [ ${mode} = "sp" ]; then

        CUDA_VISIBLE_DEVICES=0 python3 tools/train.py --config_file "config/icdar2015_resnet18_FPN_DBhead_polyLR.yaml"
    else
    
    fi
done
